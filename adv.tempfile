<html>
 <head>
    <title>Just Begun</title>
  <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
      
      <script type="text/javascript">
    
window.onscroll = changePos;

function changePos() {
    var menu = document.getElementById("navmenu");
   
    if (window.pageYOffset > 300) 
    {
        menu.style.position = "fixed";
        menu.style.top = "0";
    } else {
        menu.style.position = "";
        menu.style.top = "";
    }
}
</script>

  <div class="container-fluid">
    <!-- This row is for the GETTYBURG COLLEGE -->
    <div class="row" style="background-color:rgba(0, 0, 204, 0.8);">
      <div class="col-xs-12 col-md-12">
        <a href="/index">
            <img src="/photos/Gburg G.jpg">
        </a> 
        <h1>Gettysburg College 3D Printing Camp!</h1>
      </div>
    </div>

    <!-- This row  is for the Summer 2016 -->
   <div class="row" style="background-color:rgba(255, 153, 0, 1);">
    <div class="col-xs-12 col-md-12">
      <br>
      <h3><b>Summer 2016</b></h3>
    </div>
   </div>

    <!-- This row is for the nav menu -->
   <div class="row">
    <div class="col-xs-12 col-md-12" id="navmenu">
      <nav class="navbar navbar-inverse">
        <div class="navbar-header">
          <a class="navbar-brand" href="/index">
            <img src="/photos/Gburg G2.jpg">
          </a>
        </div>
        <ul class="nav navbar-nav">
          <li><a href="/index">Home</a></li>
          <li><a href="/about">About Us</a></li>
          <li><a href="/teachers">Teachers</a></li>
          <li><a href="#">Page 3</a></li>
        </ul>
      </nav>
    </div>
   </div>

  </div> <!-- End container for rows -->
      
      
 </head>
 <body>
  <!--container fluid to use rows -->
<div class="container-fluid">
  <div class="row">

    <!-- To hold the login/create forms -->
    <div class="col-xs-5 col-md-5">

      <!-- START OF FORM -->
      <form id="signup">
      <h1>Sign Up Form</h1>
        <p>
          <input type = "text" placeholder="Full Name" name = "fullname" id= "fullname">
        </p>
        <p>
          <input type = "text" placeholder="Email" name = "emailField" id= "emailField">
        </p>
        <p>
          <input type ="password" placeholder="Password" name = "passwordField" id= "passwordField">
        </p>
        <br><br>
        <input type="submit" value="Submit" />
      </form>
    </div>

    <!-- To hold the picture slideshow -->
    <div class="col-xs-7 col-md-7">

      <!-- Home Page SlideShow -->
      <div class="carousel slide" data-ride="carousel" data-interval="4000" data-pause="hover">

        <!-- Wrapper for slides -->
        <div class="carousel-inner">
          #set($count = 0)
          #foreach($con in $dotcontent.pull("+contentType:HomePagephotoGallery +(conhost:886ab138-41c4-4b68-9c1c-2973afcca964 conhost:SYSTEM_HOST)",10,"modDate desc"))

            #if($count == 0)
              <div class="item active">
            #else
              <div class="item">
            #end
                <img src="/contentAsset/image/$con.pic.map.identifier/fileAsset/filter/Resize/resize_w/500"/>
              </div>
            #set($count = $count + 1)
          #end
        </div> <!-- Ends inner carousel -->
      </div> <!-- Ends Carousel -->
    </div>
  </div> <!-- Ends Row -->
</div> <!-- Ends Container-fluid -->
 </body>
</html>
